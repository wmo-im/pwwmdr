  <skos:Collection rdf:about="http://codes.wmo.int/wmdr/DataPolicy">
    <dct:description>Data policy/use constraints</dct:description>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
