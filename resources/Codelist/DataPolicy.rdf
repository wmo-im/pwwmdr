        <rdfs:label>unknown</rdfs:label>
        <skos:notation>unknown</skos:notation>
      </skos:Concept>
    </skos:member>
    <skos:member>
      <skos:Concept rdf:about="http://codes.wmo.int/wmdr/DataPolicy/WMOEssential">
        <dct:description xml:lang="en">WMO Essential Data: free and unrestricted international exchange of basic data and products.</dct:description>
