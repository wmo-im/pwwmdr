    <skos:member>
      <skos:Concept rdf:about="http://codes.wmo.int/wmdr/SurfaceCoverClassification/igbp">
        <dct:description xml:lang="en">International Geosphere-Biosphere Programme land cover classification scheme</dct:description>
        <rdfs:label>Surface cover types (IGBP)</rdfs:label>
        <skos:notation>igbp</skos:notation>
      </skos:Concept>
    </skos:member>
    <skos:member>
      <skos:Concept rdf:about="http://codes.wmo.int/wmdr/SurfaceCoverClassification/lccs">
        <dct:description xml:lang="en">FAO/UNEP land cover classification scheme (LCCS), a comprehensive, standardized a priori classification system, independent of the scale or means used to map. The system can be used for any land cover classification initiative anywhere in the world, using a set of independent diagnostic criteria that allow correlation with existing classifications and legends.</dct:description>
        <rdfs:label>Surface cover types (LCCS)</rdfs:label>
        <skos:notation>lccs</skos:notation>
      </skos:Concept>
    </skos:member>
    <ldp:hasMemberRelation rdf:resource="http://www.w3.org/2004/02/skos/core#member"/>
    <dct:description>Surface cover classification scheme</dct:description>
    <skos:member>
      <skos:Concept rdf:about="http://codes.wmo.int/wmdr/SurfaceCoverClassification/globCover2009">
        <dct:description xml:lang="en">GlobCover 2009 land cover map is derived by an automatic and regionally-tuned classification of a time series of global MERIS FR mosaics for the year 2009. The global land cover map counts 22 land cover classes defined with the United Nations (UN) Land Cover Classification System (LCCS).</dct:description>
        <rdfs:label>Surface cover types (GlobCover2009)</rdfs:label>
        <skos:notation>globCover2009</skos:notation>
      </skos:Concept>
    </skos:member>
    <skos:member>
      <skos:Concept rdf:about="http://codes.wmo.int/wmdr/SurfaceCoverClassification/inapplicable">
