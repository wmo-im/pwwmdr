  <ldp:Container rdf:about="http://codes.wmo.int/wmdr/SurfaceCoverClassification">
    <skos:member>
      <skos:Concept rdf:about="http://codes.wmo.int/wmdr/SurfaceCoverClassification/unknown">
        <dct:description xml:lang="en">The Surface cover classification scheme is unknown for this particular surface cover observation.</dct:description>
